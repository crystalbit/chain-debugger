{
  "name": "fluence-test",
  "version": "1.0.57",
  "main": "build/public/electron.js",
  "homepage": "./",
  "license": "MIT",
  "files": [
    "bin/",
    "build/",
    "public/electron.ts",
    "public/preload.ts"
  ],
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.6",
    "@mui/material": "^6.4.6",
    "electron": "34.3.0",
    "electron-is-dev": "3.0.1",
    "web-vitals": "^4.2.4"
  },
  "devDependencies": {
    "@types/electron": "^1.6.12",
    "@types/node": "22.13.8",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "concurrently": "9.1.2",
    "electron-builder": "25.1.8",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-scripts": "5.0.1",
    "typescript": "5.8.2",
    "wait-on": "8.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && tsc public/electron.ts --outDir build --module commonjs --esModuleInterop true && tsc public/preload.ts --outDir build --module commonjs --esModuleInterop true && tsc bin/cli.ts --outDir build/bin --module commonjs --esModuleInterop true --skipLibCheck",
    "eject": "react-scripts eject",
    "electron-dev": "rm -rf build && yarn tsc --project tsconfig.json && concurrently -k \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "electron-builder -c.extraMetadata.main=build/electron.js",
    "preelectron-pack": "yarn build",
    "prepack": "yarn build",
    "prepare": "yarn build"
  },
  "bin": {
    "fluence-test": "./build/bin/cli.js"
  },
  "build": {
    "appId": "com.fluence.test-framework",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "dist"
    },
    "extraResources": [
      {
        "from": "build",
        "to": "build",
        "filter": [
          "**/*"
        ]
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
